public class Main {
    public static void main(String[] args) {
        char grade = 'A';
        switch (grade) {

            case 'A':
                System.out.println("Excellent - You Passed the Exam");
                break;
            case 'B':
                System.out.println("Great - You Passed the Exam");
                break;
            case 'C':
                System.out.println("Not Bad - You Passed the Exam");
                break;
            case 'D':
                System.out.println("You're So Close - You Failed the Exam");
                break;
            case 'E':
                System.out.println("You Failed - You Failed the Exam");
                break;
            case 'F':
                System.out.println("You Need to Study Hard - You Failed the Exam");
                break;
            default:
                System.out.println("You Made a Wrong Transaction.");

        }
    }
}
